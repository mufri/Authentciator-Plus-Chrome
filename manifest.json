  "content_security_policy": "script-src 'self' 'unsafe-eval' https://apis.google.com https://ssl.google-analytics.com; object-src 'self'",
  "permissions": [
    "contextMenus", "activeTab", "storage", "identity"
  ],
   "key": "",
  "oauth2": {
    "client_id": "750984674908-4nfidhu27id7u5i16iumke939689mf4o.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  }
}
