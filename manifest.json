  "author": "Mufri",
  "options_page": "html/options.html",
  "icons": {
    "128": "img/icon_128.png",
    "16": "img/icon_16.png",
    "48": "img/icon_48.png"
  },
  "minimum_chrome_version": "29",
  "browser_action": {
    "default_title": "Send to Authenticator Plus",
